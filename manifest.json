{
	"manifest_version": 2,
	"name": "Whistle",
	"version": "0.1",
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"browser_action": {
		"default_icon": "img/ok.png",
		"default_popup": "html/popup.html"
	},
	"web_accessible_resources": [
		"conf.json"
	],
	"background": {
		"scripts": [
			"script/jquery-3.2.1.min.js",
			"script/cookie.js",
			"script/count.js"
		]
	},
	"permissions": [
		"cookies",
		"alarms",
		"http://steamcommunity.com/"
	]
}
