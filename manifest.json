{
	"manifest_version": 2,
	"name": "Steam Alert",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"script/jquery-3.2.1.min.js",
				"script/alert.js"
			],
			"run_at": "document_end"
		}
	],
	"browser_action": {
		"default_icon": "img/ok.png",
		"default_popup": "html/popup.html"
	},
	"web_accessible_resources": [
		"metal.json"
	]
}
